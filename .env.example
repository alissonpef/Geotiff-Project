# Server configuration
PORT=3001

# Data directory (where .tif files are located)
DATA_DIR=./data

# GeoTIFF cache settings (in minutes)
CACHE_AGE_MINUTES=60

# CORS - allowed origin for cross-origin requests
CORS_ORIGIN=*

# Default GeoTIFF file (used when tiffId is not specified in URL)
DEFAULT_GEOTIFF=name.tif

# ------------------------
# Vari√°veis para testes locais (opcionais)
# TEST_TIFFID pode ser um nome do arquivo ou deixado vazio para usar DEFAULT_GEOTIFF

