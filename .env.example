# Configuração do servidor
PORT=3001

# Diretório de dados (onde estão os arquivos .tif)
DATA_DIR=./data

# Diretório de mídia (compatibilidade com padrão Python)
# MEDIA_DIR=/caminho/alternativo

# Configurações de cache do GeoTIFF
MAX_CACHE_SIZE_MB=1024
MAX_CACHE_AGE_HOURS=24

# Configurações de tiles
TILE_SIZE=256
MAX_ZOOM=22

# CORS
CORS_ORIGIN=*

# Ambiente (development, production)
NODE_ENV=development
